<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="4" parallel="tests" name="ECS Sanity Suite">
  <parameter name="env" value="ECS_TEST"/>
  <parameter name="browser" value="Chrome"/>
  <listeners>
    <listener class-name="framework.listeners.DynamicDependencyTransformer"/>
  </listeners>
  <test thread-count="4" name="Navigate Make A Payment tests">
    <classes>
      <class name="tests.web.ECSScripts">
        <methods>
          <include name="TC03_navigateToMakeAPaymentPage"/>
        </methods>
      </class> <!-- tests.web.ECSScripts -->
    </classes>
  </test> <!-- Navigate Make A Payment tests -->
  <test thread-count="4" name="Navigate Statement tests">
    <classes>
      <class name="tests.web.ECSScripts">
        <methods>
          <include name="TC05_navigateToStatementsPage"/>
        </methods>
      </class> <!-- tests.web.ECSScripts -->
    </classes>
  </test> <!-- Navigate Statement tests -->
</suite> <!-- ECS Sanity Suite -->
